<a {{#if href}}href="/{{href}}"{{/if}} id="{{id}}" class="manuPoint">
    <img class="iconPoint1" src="{{icon}}" alt="input">
    {{{text}}}
    {{#if readcount}}
        {{#if count}}
            <div id="readCount" class="readCount">
                {{count}}
            </div>
        {{else}}
            <div id="readCount" class="readCountNone">
                {{count}}
            </div>
        {{/if}}
    {{/if}}
</a>